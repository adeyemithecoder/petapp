# JWT secret key for signing tokens
JWT_SECRET=your_jwt_secret_here

# MongoDB connection string
MONGODB_URL=mongodb+srv://username:password@cluster.mongodb.net/dbname?retryWrites=true&w=majority&appName=your-app-name

# Alternative database URL (if used)
DATABASE_URL=mongodb+srv://username:password@cluster.mongodb.net/dbname?retryWrites=true&w=majority&appName=your-app-name

# Email credentials for sending mails
EMAIL_USER=your_email@example.com
EMAIL_PASS=your_email_app_password
EMAIL_HOST=smtp.example.com

# Application port
port=8000

# Cloudinary configuration
CLOUDINARY_NAME=your_cloudinary_name
CLOUDINARY_KEY=your_cloudinary_api_key
CLOUDINARY_SECRET=your_cloudinary_api_secret
